{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "node_modules/**/*.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "typings.d.ts"
  ],
  "exclude": [
    "node_modules/**/*.test.ts"
  ]
}
